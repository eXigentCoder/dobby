{
  "name": "dobby-example",
  "version": "0.0.12",
  "type": "module",
  "description": "An example library to demonstrate how everything fits together",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "jest --collect-coverage",
    "build": "tsc",
    "lint": "npm run lint-ci -- --fix",
